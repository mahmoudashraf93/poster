# Meta App credentials
IG_APP_ID=
IG_APP_SECRET=

# Long-lived Instagram User access token
IG_ACCESS_TOKEN=

# Profile name (default: "default")
IG_PROFILE=default

# Facebook Page ID connected to Instagram Business account
IG_PAGE_ID=

# Meta Business ID (for owned pages lookup)
IG_BUSINESS_ID=

# Instagram Business/User ID (can be fetched via `poster account`)
IG_USER_ID=

# Graph API version (default v19.0)
IG_GRAPH_VERSION=v19.0

# Polling settings for media processing
IG_POLL_INTERVAL=5s
IG_POLL_TIMEOUT=300s

# Keyring backend (auto|keychain|file)
POSTER_KEYRING_BACKEND=

# Password for encrypted file backend (non-interactive)
POSTER_KEYRING_PASSWORD=
